<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<manifest>
    <application name="StackedView">
        <crown name="StackedView">
            <nodePath>StackedViewSample</nodePath>
            <desc>user script</desc>
            <serves>
                <event name="OnStateChanged_StackedView1">
                    <nodePath>StackedViewSample.OnStateChanged_StackedView1</nodePath>
                    <param name="state" type="string" multiplicity="1" desc=""/>
                </event>
                <event name="OnStateChanged_StackedView2">
                    <nodePath>StackedViewSample.OnStateChanged_StackedView2</nodePath>
                    <param name="state" type="string" multiplicity="1" desc=""/>
                </event>
                <function name="getState_StackedView1">
                    <nodePath>StackedViewSample.getState_StackedView1</nodePath>
                    <desc></desc>
                    <return name="state" type="string" multiplicity="1" desc=""/>
                </function>
                <function name="getState_StackedView2">
                    <nodePath>StackedViewSample.getState_StackedView2</nodePath>
                    <desc></desc>
                    <return name="state" type="string" multiplicity="1" desc=""/>
                </function>
            </serves>
        </crown>
        <meta key="author">Mikolaj Rdzanek</meta>
        <meta key="version">1.0</meta>
        <meta key="priority">low</meta>
        <meta key="copy-protected">false</meta>
        <meta key="read-protected">false</meta>
        <entry path="scripts" default="main.lua"/>
    </application>
</manifest>
